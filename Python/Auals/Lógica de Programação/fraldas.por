programa {
  funcao inicio() {
    real fraldas
    inteiro quantidade
    inteiro mes
    escreva("Qual é o valor das fraldas? ")
    leia(fraldas)
    escreva("Quantas faltas são utilizadas? ")
    leia(quantidade)
    escreva("Por quantos dias será utilizada? ")
    leia(mes)
    escreva(fraldas*quantidade)
  }
}
